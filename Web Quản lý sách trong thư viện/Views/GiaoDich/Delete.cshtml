@model Quản_Lý_Sách.Models.GiaoDich

@{
    ViewBag.Title = "Xóa giao dịch";
}

<h2>Xóa giao dịch</h2>

<p>Bạn có chắc chắn muốn xóa giao dịch <strong>@Model.MaGD</strong>?</p>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(model => model.MaGD)

    <button type="submit" class="btn btn-danger">Xóa</button>
    @Html.ActionLink("Quay lại", "Index")
}
